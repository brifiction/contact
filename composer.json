{
    "name": "brifiction/entwurfhaus-contact",
    "description": "a simple contact form",
    "type": "library",
    "require": {
        "php" : "^7.2",
        "laravel/framework": "^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "brian ng",
            "email": "brianngchongeu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "brifiction\\EntwurfhausContact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "brifiction\\EntwurfhausContact\\Test\\": "tests",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "brifiction\\EntwurfhausContact\\EntwurfhausContactServiceProvider"
            ]
        }
    }
}
